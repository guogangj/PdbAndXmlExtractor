<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <Version>1.0.3</Version>
    <Description>A small tool to extract xml and pdb files from Nuget Packages.</Description>
    <PackageProjectUrl>https://github.com/guogangj/PdbAndXmlExtractor</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Update="config.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
